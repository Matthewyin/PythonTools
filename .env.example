# Office转PDF工具配置文件示例
# 复制此文件为.env并根据需要修改配置

# 转换超时时间（秒）
CONVERSION_TIMEOUT=300

# 默认输出目录（可选）
DEFAULT_OUTPUT_DIR=/output/files/path

# 日志配置
LOG_LEVEL=INFO
LOG_DIR=logs

# 文件处理配置
SKIP_TEMP_FILES=true
OVERWRITE_EXISTING=false

# 并发转换数量（暂未实现）
MAX_CONCURRENT_CONVERSIONS=1

# PDF质量设置 (low, default, high)
PDF_QUALITY=default
